article .col-lg-12 { font-size:15px; color:#5b5b5b; line-height:26px;}
article .col-lg-12 p { margin-bottom:20px;}
article .col-lg-12 .text-center { display:block; margin:4% 0px; font-size:30px;} 
.article .col-lg-12 .text-center { display:block; margin:4% 0px; font-size:30px;} 

