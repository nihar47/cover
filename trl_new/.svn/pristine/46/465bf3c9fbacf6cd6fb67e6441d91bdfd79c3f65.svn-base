}
.page-header .navbar-collapse .btn:hover { color:#00547e;}


.search_form { display:inline-block; position:relative;  }
.search_form .inner_search_form  { position:absolute; background:#0e76bc; width:356%; display:block;}
.search_form .inner_search_form  { display:block; padding:4px; visibility:visible;}
.search_form .inner_search_form .form-control { padding:6px;}
.search_form .inner_search_form .form-group { background:#fff; padding:4px 0px 4px 4px; margin:0px;}
.search_form .inner_search_form .col-sm-1 {padding:4px 4px 4px 4px; }

.search_form .inner_search_form  .btn { width:auto;  background:url(../images/search_button.png) 0 -2px no-repeat; margin-left:5px; border:0px; padding:17px; background-size:cover; border-radius:0px; margin:0px;}
.search_form .inner_search_form input[type="text"] { border-radius:0px; font:bold 12px 'Open Sans', sans-serif; color:#9a9a9a;  }



/*Start header*/

/*Start business search form*/
.business_search_form { width:100%; display:block; position:absolute; top:16%; left:0px; z-index:999; clear:both;}
.business_search_form form { margin:0 auto; width:75%; text-align:center; position:relative; padding:29.5% 5px 0px; }
.business_search_form form .form-group { margin:0px;}
.business_search_form form .col-md-5 { padding:0px; width:42.7%;}
.business_search_form form .col-md-6 { padding:0px; }

.business_search_form form input { border-radius:0px; height:auto; border-color:#d2d2d2;  font:bold 15px 'Open Sans', sans-serif; color:#9a9a9a; padding:4.4% 6px; /*box-shadow:0 1px 2px #d2d2d2 inset;*/}

.business_search_form form .pull-right input[type="text"] {padding:3.8% 6px;}

.business_search_form form input.pull-right { width:72.5%;}
.business_search_form form input[type="button"] { width:auto;  background:url(../images/search_button.png) 0 0 no-repeat; margin-left:5px; border:0px; padding:6.4% 6%; background-size:cover;}
.business_search_form img { position:absolute; left:0; right:0; margin:0 auto; max-width:100%; top:0;}

.form_inner { width:96%; overflow:hidden; margin:0 auto;}
/*End business search form*/

/*Start About us*/
.toprated_map_not { text-align:center; padding:30px 21%; font-size:20px; line-height:26px; font-weight:bold; color:#5a5a5a; background:#fafafa;}
.toprated_map_not strong { color:#1d8fcb;}
.blue {color:#1d8fcb;}
article, .btm_border { background:url(../images/container_btm_bg.png) left bottom repeat-x; padding-bottom:40px; min-height:400px; height:auto !important;}
.article, .btm_border { background:url(../images/container_btm_bg.png) left bottom repeat-x; padding-bottom:40px; min-height:400px; height:auto !important;}
.about_page { color:#5b5b5b; line-height:26px;  margin-top:35px;} 
.about_page .col-md-7 { padding:0px;}

.about_page .text-center { display:block; margin:4% 0px; font-size:22px;} 
.rating_benifits .col-lg-6 { padding:0px;}
.rating_benifits dl  { clear:both; min-height:100px; height:auto !important; }
.rating_benifits dl dt { float:left; width:17%;}
.rating_benifits dl dd { margin-left:17%; font-weight:bold; color:#7b7b7b; font-size:16px; padding-right:30px; padding-top:5px;} 
.rating_benifits dl dd strong { font-size:22px;}
.rating_benifits dl dd b { font-size:22px; color:#81b723;}

/*End About us*/

/*Landing page*/
