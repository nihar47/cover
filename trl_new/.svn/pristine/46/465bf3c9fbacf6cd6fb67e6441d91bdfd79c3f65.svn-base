@charset "utf-8";
/* CSS Document */
/*Start header*/
a:hover { color:#0e76bc !important;}
.bg_none { background:none !important; }
.border_btmnone { border-bottom:0px; box-shadow:none;}
.clear { clear: both; display: block; font-size: 0px; height: 0px; line-height: 0; width: 100%; overflow:hidden; }
.wrapper { width:100%; height:100%; display:block; overflow:hidden; margin:0 auto; font-family:'Open Sans', sans-serif;}
.page-header h1 { margin:-5px 0px 0px 0px; position:absolute; z-index:9; }
.home_slider { max-width:2000px;}
.page-header .col-sm-3 { margin-left:85px; margin-top:5px; padding:0px; z-index:9999;}
.page-header .col-sm-3 a .fa-caret-down { display:none; margin-left:5px;} 
.page-header .search_form a {font:bold 16px 'Open Sans', sans-serif; color:#424141; margin-right:10px; padding:20px 10px 20px 10px; display: inline-block;}
.page-header .search_form a.active {background:#0e76bc; color:#fff !important;}
.page-header .search_form a.active .fa-caret-down { display:inline-block; color:#fff;} 
.page-header .search_form a.active .fa-caret-right { display:none;}
.page-header .search_form a:hover { background:#0e76bc; color:#fff !important;}
.page-header .search_form:hover a { background:#0e76bc; color:#fff !important;}
.page-header .search_form:hover .fa-caret-down { display:inline-block; color:#fff;} 
.page-header .search_form:hover .fa-caret-right { display:none;}
.page-header .search_form a .fa-caret-right { margin-left:10px;}

.page-header .navbar-toggle { background:#000;}
.page-header .navbar-toggle span { background:#fff;}
.page-header .navbar-collapse { padding:0px;}
.page-header .navbar-collapse .navbar-form { padding:0px; margin:15px 0px 0px 0px;}
.page-header .navbar-collapse a { font:bold 16px 'Open Sans', sans-serif; color:#424141; padding-right:18px;}
.page-header .navbar-collapse .btn { border-radius:0px; border:0px; padding:11px 12px 10px; 
background: #2db2db; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #2db2db 2%, #2db2db 6%, #249ad0 7%, #0e76bc 100%); /* FF3.6+ */
